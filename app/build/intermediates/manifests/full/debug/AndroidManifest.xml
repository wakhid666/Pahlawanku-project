<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.riskimidiw.kuispahlawan"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/icn"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.kuispahlawan.SplashActivity"
            android:screenOrientation="userPortrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kuispahlawan.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.kuispahlawan.BiodataActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.kuispahlawan.QuizActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name="com.example.kuispahlawan.AnswerActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name="com.example.kuispahlawan.ScoreActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name="com.example.kuispahlawan.AboutActivity"
            android:screenOrientation="userPortrait" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
    </application>

</manifest>