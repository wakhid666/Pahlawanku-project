<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kuispahlawan">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icn"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.kuispahlawan.SplashActivity"
            android:screenOrientation="userPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kuispahlawan.MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.kuispahlawan.BiodataActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.kuispahlawan.QuizActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name="com.example.kuispahlawan.AnswerActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name="com.example.kuispahlawan.ScoreActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name="com.example.kuispahlawan.AboutActivity"
            android:screenOrientation="userPortrait" />

    </application>

</manifest>